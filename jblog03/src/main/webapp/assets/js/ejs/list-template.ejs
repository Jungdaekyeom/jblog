<%
	  for(i=0; i < data.length; i++){
	 	var vo = data[i];
	  	if(vo.count == null){
		  	vo.count = '';
	  }
%>
		<tr value='<%=vo.no%>'>
			<td><%= i+1 %></td>
			<td><%= vo.name %></td>
			<td><%= vo.count %></td>
			<td><%= vo.description %></td>
			<td>
		<%
			
		  	if(vo.name=='미분류'){
		%>
				&nbsp;
		<%
			} else {
		%>
			<a href="<%= path %>/blog/<%= vo.blogId %>/admin/category/delete/<%=vo.no%>">
				<img src="<%= path %>/assets/images/delete.jpg">
			</a>
		<%
			}
		%>
			</td>	
		</tr>
<%
	}
%>